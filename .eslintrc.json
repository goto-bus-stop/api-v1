{
  "extends": "airbnb-base",
  "parser": "babel-eslint",

  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-param-reassign": ["error", { "props": false }],
    "generator-star-spacing": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
